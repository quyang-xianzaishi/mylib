<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/activity_register_success"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@color/white"
  android:orientation="vertical">

  <TextView
    android:layout_width="match_parent"
    android:background="@color/chead"
    android:layout_height="23dp" />

  <FrameLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:visibility="visible">

    <TextView
      android:layout_width="match_parent"
      android:layout_height="44dp"
      android:gravity="center"
      android:text=""
      android:textColor="#000"
      android:textSize="17sp" />

    <ImageView
      android:id="@+id/iv_back"
      android:layout_width="22dp"
      android:layout_height="18dp"
      android:layout_marginLeft="15dp"
      android:layout_gravity="center_vertical"
      android:src="@mipmap/icon_back_gray"
      android:visibility="gone" />

    <TextView
      android:id="@+id/tv_back"
      android:layout_width="40dp"
      android:layout_height="24dp"
      android:layout_marginLeft="12dp"

      android:layout_gravity="center_vertical"
      android:background="@color/transparenc_all"
      android:visibility="gone" />

  </FrameLayout>
  <TextView
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="@color/cf6f6f6" />

  <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="70dp"
    android:gravity="center_horizontal">

    <ImageView
      android:id="@+id/icon"
      android:layout_width="match_parent"
      android:layout_height="46dp"
      android:layout_marginLeft="98dp"
      android:layout_marginRight="98dp"
      android:layout_centerHorizontal="true"
      android:src="@mipmap/logo_new" />

    <TextView
      android:id="@+id/tip_next"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="94dp"
      android:layout_below="@+id/icon"
      android:gravity="center"
      android:text="@string/active_two_comfim_info"
      android:textColor="@color/c666"
      android:textSize="11sp" />

    <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="40dp"
      android:layout_below="@+id/tip_next"
      android:orientation="vertical">

      <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:gravity="center"
        android:text="@string/six_pwd"
        android:textColor="@color/light_gray"
        android:textSize="9sp"
        android:visibility="gone" />

      <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal">

        <EditText
          android:id="@+id/et_pwd_one"
          android:layout_width="25dp"
          android:layout_height="46dp"
          android:theme="@style/MyEditText"
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:textColor="#171c61"
          android:gravity="center"
          android:inputType="numberPassword" />


        <EditText
          android:id="@+id/et_pwd_two"
          android:layout_width="25dp"
          android:layout_height="46dp"
          android:layout_marginLeft="10dp"
          android:theme="@style/MyEditText"
          android:layout_marginRight="10dp"
          android:gravity="center"
          android:textColor="#171c61"
          android:inputType="numberPassword" />


        <EditText
          android:id="@+id/et_pwd_three"
          android:layout_width="25dp"
          android:layout_height="46dp"
          android:layout_marginLeft="10dp"
          android:theme="@style/MyEditText"
          android:textColor="#171c61"
          android:layout_marginRight="10dp"
          android:gravity="center"
          android:inputType="numberPassword" />


        <EditText
          android:id="@+id/et_pwd_four"
          android:layout_width="25dp"
          android:layout_height="46dp"
          android:theme="@style/MyEditText"
          android:layout_marginLeft="10dp"
          android:textColor="#171c61"
          android:layout_marginRight="10dp"
          android:gravity="center"
          android:inputType="numberPassword" />


        <EditText
          android:id="@+id/et_pwd_five"
          android:layout_width="25dp"
          android:theme="@style/MyEditText"
          android:layout_height="46dp"
          android:textColor="#171c61"
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:gravity="center"
          android:inputType="numberPassword" />

        <EditText
          android:id="@+id/et_pwd_six"
          android:layout_width="25dp"
          android:layout_height="46dp"
          android:textColor="#171c61"
          android:layout_marginLeft="10dp"
          android:layout_marginRight="10dp"
          android:gravity="center"
          android:theme="@style/MyEditText"
          android:inputType="numberPassword" />

      </LinearLayout>

      <Button
        android:id="@+id/btn_next"
        android:layout_width="131dp"
        android:layout_height="39dp"
        android:layout_marginTop="90dp"
        android:layout_gravity="center_horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:background="@drawable/round_blue"
        android:text="@string/btn_ok"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:visibility="gone" />

    </LinearLayout>


  </RelativeLayout>


</LinearLayout>
