<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/root"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:orientation="vertical">

  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:orientation="horizontal">

    <TextView
      android:id="@+id/tv_date"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="92dp"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_order_id"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="92dp"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_trade_vip"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="92dp"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_trade_type"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="92dp"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_seeds"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="92dp"
      android:textColor="@color/cEA5412"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_growing_seeds"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="130dp"
      android:textColor="@color/cEA5412"
      android:layout_height="wrap_content" />

    <TextView
      android:id="@+id/tv_trade_seeds"
      style="@style/dream_package_item_text_style_three"
      android:layout_width="130dp"
      android:textColor="@color/cEA5412"
      android:layout_height="wrap_content" />

  </LinearLayout>


</LinearLayout>