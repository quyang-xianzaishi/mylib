<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/activity_credit_grade_system"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical"
  tools:context="com.example.administrator.lubanone.activity.home.CreditGradeSystemActivity">

  <FrameLayout
    android:id="@+id/one"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="25dp"
    android:background="@color/title_background_blue">

    <TextView
      android:layout_width="match_parent"
      android:layout_height="44dp"
      android:gravity="center"
      android:text="@string/credit_grade_system"
      android:textColor="@color/c333"
      android:textSize="18sp"
      android:background="@color/white"/>

    <ImageView
      android:id="@+id/iv_back"
      android:layout_width="22dp"
      android:layout_height="18.5dp"
      android:layout_marginLeft="15dp"
      android:layout_gravity="center_vertical"
      android:src="@mipmap/icon_back_gray" />

    <TextView
      android:id="@+id/tv_back"
      android:layout_width="40dp"
      android:layout_height="30dp"
      android:layout_centerVertical="true"
      android:background="@color/transparenc_all" />


  </FrameLayout>


  <include
    android:id="@id/empty_layout"
    android:visibility="gone"
    layout="@layout/empty_layout" />
<!--


  <TextView
    android:id="@+id/tv_info"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="20dp"
    android:paddingBottom="20dp"
    android:paddingLeft="17dp"
    android:paddingRight="17dp"
    android:textColor="@color/c000"
    android:textSize="17sp" />
-->


  <WebView
    android:layout_marginTop="5dp"
    android:id="@+id/webView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

  </WebView>
</LinearLayout>
